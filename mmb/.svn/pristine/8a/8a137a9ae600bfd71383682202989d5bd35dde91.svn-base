
/*商品列表*/

.product-list>.media>a{
    border-bottom: 1px solid #ccc;
    text-decoration: none;
    display: block;
    padding: 10px;
}
.product-list>.media>a>.pic{
    float: left;
    display: inline-block;
    vertical-align: middle;
}
.product-list>.media>a>.media-body{
    font-size: 14px;
    line-height: 24px;
    padding-left: 10px;

}
.product-list>.media>a>.media-body>.tit{
    margin-bottom: 35px;
}
.product-list>.media>a>.media-body>.price>em{
    font-size: 18px;
    color: #ea3900;
    vertical-align: middle;

}

.media-body .other{

    color: #a1a1a1;
    font-size: 12px;
}
.media-body .other>em:first-child{
    margin-right: 10px;
}

/*面包屑*/
.breadcrumb{
    margin-bottom: 0px;
}

.breadcrumb > li + li:before {
    color: #000;
    font-size: 16px;
    content: ">\00a0";
}
.btn{
    padding: 2px 15px;
    float: right;
}
.btn-default {
    background-color: #EBEBF0;

}

/*上下页*/
.page {
    margin-top: 10px !important;
    text-align: center;
}
.page >.w33 {
    width: 33.33%;
    float: left;
    text-align: center;
}
.page a {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
    background-color: #d4d4d4;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#f1f1f1), to(#d4d4d4));
    background: -moz-linear-gradient(top, #f1f1f1, #d4d4d4);
    background: -o-linear-gradient(top, #f1f1f1, #d4d4d4);
    border-bottom: 1px solid #bababa;
    box-shadow: 0 0 1px #666;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
    text-align: center;
    font-size: 14px;
    line-height: 32px;
    overflow: hidden;
    padding: 8px 20px;
    /* margin: 0 5px; */
}