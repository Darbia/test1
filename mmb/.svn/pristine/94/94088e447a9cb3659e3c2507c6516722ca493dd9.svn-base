@charset "UTF-8";
.mmb-main > .row {
  border-right: 1px solid #EEEEEE;
}
.mmb-goods {
  padding: 10px 20px;
  border-left: 1px solid #EEEEEE;
  border-bottom: 1px solid #EEEEEE;
  height: 270px;
}
.mmb-goods > .mmb-img {
  padding: 10px 0;
}
.mmb-goods > .mmb-img > img {
  width: 140px;
  height: 140px;
}
.mmb-goods > .mmb-title {
  height: 40px;
  margin: 0;
  color: #000;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.mmb-goods > .mmb-price {
  color: #FF841D;
}
.mmb-goods > .mmb-source {
  font-size: 12px;
  color: #A1A1A1;
}
