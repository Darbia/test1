
.panel-group .panel {
    margin-bottom: -6px;
    border-radius:0px;
}
.panel-heading{
    background: #EDEDED;
}
.panel-title > a{
    color: #000;
    font-size: 16px;
    display: block;
    font-weight: bold;
    text-decoration: none;
}
.panel-title >a>img{
    float: right;
    width: 30px;
    height:20px;
}
.panel-group .panel-heading + .panel-collapse > .panel-body{
    background: #f9f9f9;
    padding:0px;
    display: none;
}
.panel-group .panel-heading + .panel-collapse > .panel-body>.row>div{
    height: 40px;
    line-height: 40px;
    text-align: center;
    border-right: 1px solid #e0e0e0;
    border-bottom:1px solid #e0e0e0;
}
.panel-group .panel-heading + .panel-collapse > .panel-body>.row>div>a{

    display: block;
    width: 100%;
    color: #333;
}

.show{
    display: block;
}